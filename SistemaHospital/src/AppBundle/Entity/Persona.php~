namespace AppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;


/**
 *
 * @ORM\MappedSuperclass
 */
class Persona
{
    /** @ORM\Column(type="string",length=50) **/
    protected $nombre;

    /** @ORM\Column(type="string",length=50) **/
    protected $apellido;

    /** @ORM\Column(type="string",length=50) **/
    protected $genero;

    /** @ORM\Column(type="integer", unique=true) **/
    protected $dni;

    /** @ORM\Column(type="integer") **/
    protected $edad;




}