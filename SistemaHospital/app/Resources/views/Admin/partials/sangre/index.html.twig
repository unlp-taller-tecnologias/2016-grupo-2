{% extends 'Admin/layout.html.twig' %}

{% block main %}
    <h1>Listado de Tipos de Sangre</h1>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
               
                <th>Nombre</th>

            </tr>
        </thead>
        <tbody>
        {% for sangre in sangres %}
            <tr>
              
                <td>{{ sangre.nombre }}</td>

            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
